{
  "family": "maple-quest-task",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "512",
  "memory": "1024",
  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  },
  "executionRoleArn": "arn:aws:iam::508426719460:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "django-app",
      "image": "508426719460.dkr.ecr.us-west-2.amazonaws.com/maple-quest:latest",
      "portMappings": [
        {
          "containerPort": 8000,
          "protocol": "tcp",
          "name": "django-app-8000-tcp"
        }
      ],
      "essential": true,
      "environment": [
        {
          "name": "DB_NAME",
          "value": "your_db_name"
        },
        {
          "name": "DB_USER",
          "value": "postgres"
        },
        {
            "name": "USE_HTTPS",
            "value": "False"
        },
        {
          "name": "DB_PASSWORD",
          "value": "your_password"
        },
        {
          "name": "DB_HOST",
          "value": "your-rds-endpoint.us-west-2.rds.amazonaws.com"
        },
        {
          "name": "DB_PORT",
          "value": "5432"
        },
        {
          "name": "SECRET_KEY",
          "value": "your-django-secret-key-change-this"
        },
        {
          "name": "DEBUG",
          "value": "False"
        },
        {
          "name": "ALLOWED_HOSTS",
          "value": "*"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/maple-quest",
          "awslogs-create-group": "true",
          "awslogs-region": "us-west-2",
          "awslogs-stream-prefix": "django"
        }
      }
    }
  ]
}